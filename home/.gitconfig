[alias]
  ignore = update-index --assume-unchanged
  unignore = update-index --no-assume-unchanged
  ignored = !git ls-files -v | grep "^[[:lower:]]"
	integrate = !sh -c 'git push $1 $2:ready-to-merge/into-master/${2}' -
[user]
	name =
  useconfigonly = true
	email =
[push]
	default = simple
[core]
  excludesfile = /Users/alistair/.gitignore_global
  autocrlf = input
	commentchar = $
[remote "origin"]
	push = HEAD
[branch]
	autosetupmerge = true
[diff]
  compactionheuristic = true
[color]
	branch = auto
	diff = auto
	ui = true
[fetch]
  prune = true
[credential]
  helper = osxkeychain
